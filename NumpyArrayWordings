# Creating words from numpy arrays

import numpy as np

array=np.array([[["a","b","c"],["d","e","f"],["g","h","i"]],
               [["j","k","l"],["m","n","o"],["p","q","r"]],
               [["s","t","u"],["v","w","x"],[" ","y","z"]]])

word1=array[0,2,0]+array[1,1,2]+array[0,1,0]
word2=array[0,0,1]+array[2,0,2]+array[2,0,0]
word3=array[0,1,2]+array[1,1,2]+array[0,2,0]

print(word1,word2,word3)
print(array.dtype)
print(array.ndim)

